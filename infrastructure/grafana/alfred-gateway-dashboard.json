{
  "__comment": "Alfred Gateway Dashboard - Grafana template for Prometheus metrics",
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "Prometheus datasource for Alfred gateway metrics",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "annotations": { "list": [] },
  "description": "Alfred AI Gateway â€” Request rates, latency, token usage, cache, wallet ops, provider health",
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Gateway Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false,
      "panels": []
    },
    {
      "title": "Total Requests / sec",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "fieldConfig": {
        "defaults": { "unit": "reqps", "color": { "mode": "palette-classic" } }
      },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_requests_total[5m]))",
          "legendFormat": "total"
        },
        {
          "expr": "sum(rate(alfred_gateway_requests_total[5m])) by (status)",
          "legendFormat": "{{status}}"
        }
      ]
    },
    {
      "title": "P95 Latency (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "fieldConfig": {
        "defaults": { "unit": "ms", "color": { "mode": "palette-classic" } }
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(alfred_gateway_request_duration_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(alfred_gateway_request_duration_ms_bucket[5m])) by (le))",
          "legendFormat": "p50"
        }
      ]
    },
    {
      "title": "Error Rate %",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0, "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "100 * sum(rate(alfred_gateway_requests_total{status=~\"5..\"}[5m])) / sum(rate(alfred_gateway_requests_total[5m]))",
          "legendFormat": "error %"
        }
      ]
    },
    {
      "title": "Provider Health",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 9 },
      "collapsed": false
    },
    {
      "title": "Requests by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_requests_total[5m])) by (provider)",
          "legendFormat": "{{provider}}"
        }
      ]
    },
    {
      "title": "Provider Latency P95",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
      "fieldConfig": { "defaults": { "unit": "ms" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(alfred_gateway_request_duration_ms_bucket[5m])) by (le, provider))",
          "legendFormat": "{{provider}}"
        }
      ]
    },
    {
      "title": "Provider Errors",
      "type": "table",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 18 },
      "targets": [
        {
          "expr": "sum(increase(alfred_gateway_requests_total{status=~\"5..\"}[1h])) by (provider)",
          "legendFormat": "{{provider}}",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "title": "Circuit Breaker State",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 18 },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            { "type": "value", "options": { "0": { "text": "CLOSED", "color": "green" }, "1": { "text": "OPEN", "color": "red" }, "2": { "text": "HALF-OPEN", "color": "yellow" } } }
          ]
        }
      },
      "targets": [
        {
          "expr": "alfred_gateway_circuit_breaker_state",
          "legendFormat": "{{provider}}"
        }
      ]
    },
    {
      "title": "Token & Cost",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 24 },
      "collapsed": false
    },
    {
      "title": "Token Usage / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 25 },
      "fieldConfig": { "defaults": { "unit": "short" } },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_tokens_total[5m])) * 60 ",
          "legendFormat": "total tokens/min"
        },
        {
          "expr": "sum(rate(alfred_gateway_tokens_total[5m])) by (type) * 60",
          "legendFormat": "{{type}}"
        }
      ]
    },
    {
      "title": "Estimated Cost / hr ($)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 25 },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 200 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_cost_total[5m])) * 3600",
          "legendFormat": "$/hr"
        }
      ]
    },
    {
      "title": "Cache Performance",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 33 },
      "collapsed": false
    },
    {
      "title": "Semantic Cache Hit Rate %",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 34 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "min": 0, "max": 100,
          "thresholds": {
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 20 },
              { "color": "green", "value": 30 }
            ]
          }
        }
      },
      "targets": [
        {
          "expr": "100 * sum(rate(alfred_gateway_cache_hits_total[5m])) / (sum(rate(alfred_gateway_cache_hits_total[5m])) + sum(rate(alfred_gateway_cache_misses_total[5m])))",
          "legendFormat": "hit rate"
        }
      ]
    },
    {
      "title": "Cache Hits vs Misses",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 34 },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_cache_hits_total[5m]))",
          "legendFormat": "hits"
        },
        {
          "expr": "sum(rate(alfred_gateway_cache_misses_total[5m]))",
          "legendFormat": "misses"
        }
      ]
    },
    {
      "title": "Cache Size (entries)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 34 },
      "targets": [
        {
          "expr": "alfred_gateway_cache_entries",
          "legendFormat": "entries"
        }
      ]
    },
    {
      "title": "Wallet & Ledger",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 42 },
      "collapsed": false
    },
    {
      "title": "Wallet Operations / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 43 },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_wallet_ops_total[5m])) by (operation) * 60",
          "legendFormat": "{{operation}}"
        }
      ]
    },
    {
      "title": "Wallet Hard Limit Blocks",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 43 },
      "fieldConfig": { "defaults": { "color": { "fixedColor": "red", "mode": "fixed" } } },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_wallet_blocks_total[5m])) * 60",
          "legendFormat": "blocks/min"
        }
      ]
    },
    {
      "title": "Safety Pipeline",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 51 },
      "collapsed": false
    },
    {
      "title": "Safety Violations / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 52 },
      "targets": [
        {
          "expr": "sum(rate(alfred_gateway_safety_violations_total[5m])) by (category) * 60",
          "legendFormat": "{{category}}"
        }
      ]
    },
    {
      "title": "Safety Pipeline Latency (ms)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 52 },
      "fieldConfig": { "defaults": { "unit": "ms" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(alfred_safety_check_duration_ms_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ]
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "tags": ["alfred", "gateway", "llm", "ai"],
  "templating": {
    "list": [
      {
        "name": "provider",
        "type": "query",
        "query": "label_values(alfred_gateway_requests_total, provider)",
        "multi": true,
        "includeAll": true,
        "current": { "text": "All", "value": "$__all" }
      },
      {
        "name": "model",
        "type": "query",
        "query": "label_values(alfred_gateway_requests_total, model)",
        "multi": true,
        "includeAll": true,
        "current": { "text": "All", "value": "$__all" }
      }
    ]
  },
  "time": { "from": "now-6h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Alfred AI Gateway",
  "uid": "alfred-gateway-v1",
  "version": 1
}
